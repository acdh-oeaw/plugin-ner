{
  "name": "@recogito/plugin-ner",
  "version": "0.2.0",
  "description": "Recogito Studio plugin which can perform Named Entity Recognition on TEI and Plain Text.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "npm start --prefix .dev/",
    "build": "rm -rf dist && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.jpg\" dist"
  },
  "exports": {
    ".": "./dist/index.js",
    "./DocumentMenuExtension": "./dist/components/DocumentMenuExtension.js",
    "./NERAgentRoute.ts": "./dist/api/NERAgentRoute.js"
  },
  "devDependencies": {
    "@trigger.dev/build": "^3.3.17",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "astro": "^5.7.4",
    "copyfiles": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@recogito/standoff-converter": "^0.1.4",
    "@recogito/studio-sdk": "^0.6.1",
    "@trigger.dev/sdk": "^3.3.17",
    "tus-js-client": "^4.3.1",
    "uuid": "^11.1.0"
  }
}
